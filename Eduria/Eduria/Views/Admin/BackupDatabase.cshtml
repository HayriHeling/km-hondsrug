@model Eduria.Models.DatabaseModel
@{
    ViewData["Title"] = "BackupDatabase";
}
    <div id="content" class="row">
        <div class="medium-12 columns">
            <h2>Backup database</h2>
            <form>
                <p>Je database backup kan <a onclick="location.href = '@Url.Action("Download", "Admin")'">hier</a> worden gedownload.</p>
                <input type="submit" asp-controller="Dashboard" class="button blue" asp-action="Index" value="Terug naar je dashboard" />
            </form>
        </div>
    </div>